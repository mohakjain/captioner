    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.1.1",
    "jsdom": "^25.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
